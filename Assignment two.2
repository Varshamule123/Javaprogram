public class transpose 
{
    public static void main(String args[])
    {
        int a[][]={{3,7,9},{7,8,9},{4,6,9}};
        int t[][]=new int [3][3];
        {
            for(int i=0;i<3;i++)
            {
                for(int j=0;j<3;j++)
                {
                    t[i][j]=a[j][i];
                }
            }
            System.out.println("before transpose:");
            for(int i=0;i<3;i++)
            {
                for(int j=0;j<3;j++)
                {
                    System.out.println(a[i][j]+" ");
                }
                System.out.println();
            }
            System.out.println("after tranpose:");
            for(int i= 0;i<3;i++)
            {
                for(int j=0;j<3;j++)
                {
                    System.out.println(t[i][j]+" ");
                }
                System.out.println();
            }
        }
    }
}
